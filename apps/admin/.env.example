# +JUNTOS - Admin Panel Environment Variables
# Copy this file to .env and adjust the values for your environment

# API Configuration
VITE_API_BASE_URL=http://localhost:8080
VITE_API_TIMEOUT=10000

# Authentication (for future admin login)
VITE_JWT_SECRET=your-admin-jwt-secret
VITE_JWT_EXPIRATION=3600000

# Application Settings
VITE_APP_NAME=+JUNTOS Admin
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# UI Configuration
VITE_DEFAULT_PAGE_SIZE=25
VITE_MAX_UPLOAD_SIZE=5242880

# External Services
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
VITE_MAPBOX_ACCESS_TOKEN=your-mapbox-access-token

# Development
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_MSW=false
